{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "app/*": [
                "*"
            ],
            "src/*": [
                "src/*"
            ],
            "utils/*": [
                "src/utils/*"
            ],
            "components/*": [
                "src/components/*"
            ],
            "layouts/*": [
                "src/layouts/*"
            ],
            "pages/*": [
                "src/pages/*"
            ],
            "assets/*": [
                "src/assets/*"
            ],
            "boot/*": [
                "src/boot/*"
            ]
        },
    },
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "**/dist/**",
        "**/.quasar/**",
        "**/node_modules/**"
    ]
}
